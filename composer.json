{
    "name": "nodeloc/flarum-ext-read-permission",
    "description": "A ext for read permission",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT-Modern-Variant",
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "Nodeloc",
            "email": "james@nodeloc.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nodeloc\\ReadPermission\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "ReadPermission",
            "category": "",
            "icon": {
                "name": "fas fa-eye-slash",
                "color": "#ffffff",
                "backgroundColor": "86469C"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": true,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": true,
                "backendTesting": false,
                "editorConfig": true,
                "styleci": false
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
